{% if collection.metafields.custom_fields["private"] != blank %}
  <div class="custom-field custom-field__private custom-field__type--checkbox">
    <h3 class="custom-field--title">Make Private</h3>
    <div class="custom-field--value">
      {% if collection.metafields.custom_fields["private"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}
{% if product.metafields.custom_fields["whats_included_accent_image"] != blank %}
  <div class="custom-field custom-field__whats-included-accent-image custom-field__type--image">
    <h3 class="custom-field--title">What's Included Accent Image</h3>
    <div class="custom-field--value">  
      {% render 'cf-image' with image: product.metafields.custom_fields["whats_included_accent_image"], print: 'url' %}
    </div>
  </div>
{% endif %}